<application>
  <component name="LogHighlightingSettings">
    <highlightingPatterns>
      <LogHighlightingPattern pattern="^\s*(?:PHP\s*)?e(rror)?\s*$" stripe="true" />
      <LogHighlightingPattern pattern="^\s*(?:PHP\s*)?w(arn(ing)?)?\s*$" stripe="true" />
      <LogHighlightingPattern pattern="^\s*(?:PHP\s*)?i(nfo)?\s*$" stripe="true" />
      <LogHighlightingPattern pattern="^\s*(?:PHP\s*)?n(otice)?\s*$" stripe="true" />
      <LogHighlightingPattern pattern="^\s*(?:PHP\s*)?f(?:atal(?:\s*error)?)?\s*$" stripe="true" />
      <LogHighlightingPattern pattern="^\[([^]]+)]\s?([^.]+)\.([\w]+):(.*)$" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(ERROR):" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(WARNING):" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(INFO):" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(ALERT):" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(DEBUG):" stripe="true" />
      <LogHighlightingPattern pattern="^[^.]+\.(CRITICAL):" stripe="true" />
    </highlightingPatterns>
    <parsingPatterns>
      <LogParsingPattern name="Laravel" pattern="^\[([\d\s:-]+)\] ([^.]+)\.([^:]+): (.*)(\{*\})$" timePattern="dd-MM-yyyy HH:mm:ss" linePattern="^\[" timeId="0" severityId="2" categoryId="4" />
    </parsingPatterns>
    <settingsVersion>4</settingsVersion>
    <lastAddedDefaultFormat>3</lastAddedDefaultFormat>
  </component>
</application>